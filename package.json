{
  "name": "metalsmith-react-templates",
  "version": "5.0.2",
  "description": "A metalsmith plugin to render files using ReactJS based templates",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel ./src -d ./lib",
    "clean": "rm -rf ./lib",
    "release": "npm run build && npm run publish",
    "test": "mocha --compilers js:babel-core/register 'tests/specs/**/*.spec.@(js|jsx)'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yeojz/metalsmith-react-templates.git"
  },
  "keywords": [
    "metalsmith",
    "static-site",
    "generator",
    "react",
    "reactjs",
    "template",
    "engine",
    "view"
  ],
  "author": "Gerald Yeo <contact@fusedthought.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yeojz/metalsmith-react-templates/issues"
  },
  "homepage": "https://github.com/yeojz/metalsmith-react-templates",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "metalsmith": "^2.2.0",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.12.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "sync-exec": "^0.6.2"
  },
  "dependencies": {
    "async": "^2.0.1",
    "debug": "^2.2.0",
    "lodash.defaults": "^4.2.0",
    "lodash.foreach": "^4.5.0",
    "lodash.forown": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "multimatch": "^2.1.0"
  },
  "peerDependencies": {
    "babel-core": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "react": "^0.14.0 || ^15.0.0",
